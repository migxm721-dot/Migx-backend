{
  "expo": {
    "name": "migx",
    "slug": "migx",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo_migx.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.migxnew.app",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },

    "android": {
      "package": "com.migxnew.app",
      "minSdkVersion": 24,
      "targetSdkVersion": 35,
      "compileSdkVersion": 35,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo_migx.png",
        "backgroundColor": "#043311"
      },
      "permissions": [
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_DATA_SYNC",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.REQUEST_IGNORE_BATTERY_OPTIMIZATIONS"
      ]
    },

    "plugins": [
      "expo-router",

      [
        "expo-build-properties",
        {
          "android": {
            "ndkVersion": "26.1.10909125",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "abiFilters": ["armeabi-v7a", "arm64-v8a"]
          }
        }
      ],

      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/logo_migx.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],

      [
        "expo-notifications",
        {
          "icon": "./assets/logo/ic_migx.png",
          "color": "#043311",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "eas": {
        "projectId": "7fd7927e-18cd-4ade-82aa-c6af4073305d"
      }
    }
  }
}